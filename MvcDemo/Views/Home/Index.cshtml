<script>
    $(function () {

        $('#my-ajax-table').dynatable({
            dataset: {
                ajax: true,
                ajaxUrl: '@Url.Action("EmployeeData", "Home")',
                ajaxOnLoad: true,
                records: []
            }
        });

    });
</script>